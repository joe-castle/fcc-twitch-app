{
  "name": "twitch-app",
  "version": "1.0.0",
  "description": "Twitch App from FCC",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf build",
    "start": "NODE_ENV=development node -- src/backend/",
    "server": "NODE_ENV=test nodemon --exec node -- src/backend/",
    "db": "mongod --dbpath data/db",
    "test": "NODE_ENV=test mocha --watch",
    "lint": "eslint src/",
    "build:webpack": "NODE_ENV=production webpack --progress --colors --config webpack.prod.config.js",
    "build": "npm run clean && npm run build:webpack",
    "deploy": "git push heroku master"
  },
  "author": "Joe Smith <joesmith0488@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.1",
    "express": "^4.13.3",
    "mongoose": "^5.6.6",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.1.0",
    "redux": "^4.0.4",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.3.17",
    "babel-loader": "^8.0.6",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^3.1.0",
    "eslint": "^6.1.0",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-react": "^7.14.3",
    "html-webpack-plugin": "^3.2.0",
    "mocha": "^6.2.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.12.9",
    "redux-devtools": "^3.0.0",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "supertest": "^4.0.2",
    "webpack": "^4.38.0",
    "webpack-dev-server": "^3.7.2"
  }
}
